{
  "name": "ypmessenger",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=16.0.0"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": "defaults"
      }
    }
  },
  "scripts": {
    "prepare": "husky install",
    "build": "npx rimraf dist/* && parcel build src/index.html --no-source-maps --no-cache ",
    "dev": "npx rimraf dist/* && parcel src/index.html --no-source-maps --no-cache",
    "start": "npm run build && node ./server.js",
    "lintFix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "lint": "bash -c tsc --noEmit && eslint src --fix",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{scss, css}": "stylelint \"**/*.css\" --fix",
    "*.{ts, js}": "npm run lint"
  },
  "author": "Dmitrii Nazimov",
  "license": "MIT",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.5",
    "@babel/core": "7.17.8",
    "@babel/register": "7.18.9",
    "@types/node": "18.7.23",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "babel-core": "6.26.3",
    "eslint": "8.23.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "parcel": "2.4.0",
    "parcel-plugin-handlebars-precompile": "1.0.2",
    "parcel-reporter-static-files-copy": "1.4.0",
    "postcss": "8.4.16",
    "postcss-custom-properties": "12.1.8",
    "postcss-functions": "4.0.2",
    "postcss-import": "12.0.1",
    "postcss-mixins": "9.0.4",
    "postcss-nested": "5.0.6",
    "postcss-simple-vars": "7.0.0",
    "postcss-url": "10.1.3",
    "rimraf": "3.0.2",
    "stylelint": "14.12.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "28.0.0",
    "stylelint-scss": "4.3.0",
    "stylelint-selector-bem-pattern": "2.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "dependencies": {
    "express": "4.18.1",
    "handlebars": "4.7.7",
    "nanoid": "4.0.0"
  }
}
